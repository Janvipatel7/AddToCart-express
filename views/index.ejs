<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Add To Cart</title>
    <link rel="stylesheet" href="/assets/css/Bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/remixicon/4.6.0/remixicon.css">
    <link rel="stylesheet" href="/assets/css/style.css">
</head>

<body>

    <%- include('./partials/header.ejs') %>

        <main>
            <section>
                <div id="carouselExampleSlidesOnly" class="carousel slide" data-bs-ride="carousel">
                    <div class="carousel-inner text-center">

                        <div class="carousel-item active">
                            <img src="/assets/images/jwellery-6.jpg" class="d-block w-100 img-fluid">
                            <div class="carousel-caption">
                                <h5 class="carousel-text lh-base">
                                    Discover the <br> Best of the Best
                                </h5>
                                <button class="Shop-colle-btn mt-5">Shop Collection</button>
                            </div>
                        </div>

                        <div class="carousel-item">
                            <img src="/assets/images/jwellery-5.jpg" class="d-block w-100 img-fluid">
                            <div class="carousel-caption">
                                <h5 class="carousel-text lh-base">
                                    Oh, <br> Hello Newness!
                                </h5>
                                <button class="Shop-colle-btn mt-5">Shop Collection</button>
                            </div>
                        </div>

                        <div class="carousel-item">
                            <img src="/assets/images/jwellery-7.jpg" class="d-block w-100 img-fluid">
                            <div class="carousel-caption">
                                <h5 class="carousel-text lh-base">
                                    Discover the <br> world of jewelry
                                </h5>
                                <button class="Shop-colle-btn mt-5">Shop Collection</button>
                            </div>
                        </div>

                    </div>
                </div>
            </section>

            <section class="padding-sec">
                <div class="container">
                    <div class="flat-title style-line text-center">
                        <div class="line-title"></div>
                        <h4 class="fs-1">Discover Our Categories</h4>
                    </div>

                    <div class="row mt-4">

                        <div class="col-lg-3 col-md-4 col-sm-6 text-center">
                            <div class="hover-img product-image">
                                <img src="/assets/images/Categorie-1.jpg" class="img-fluid">
                            </div>
                            <p class="py-3 fs-4 fw-medium">Necklaces</p>
                        </div>

                        <div class="col-lg-3 col-md-4 col-sm-6 text-center">
                            <div class="product-image">
                                <img src="/assets/images/Categorie-2.jpg" class="img-fluid">
                            </div>
                            <p class="py-3 fs-4 fw-medium">Bracelets</p>
                        </div>

                        <div class="col-lg-3 col-md-4 col-sm-6 text-center">
                            <div class="product-image">
                                <img src="/assets/images/Categories-3.jpg" class="img-fluid">
                            </div>
                            <p class="py-3 fs-4 fw-medium">Earrings</p>
                        </div>

                        <div class="col-lg-3 col-md-4 col-sm-6 text-center">
                            <div class="product-image">
                                <img src="/assets/images/Categories-4.jpg" class="img-fluid">
                            </div>
                            <p class="py-3 fs-4 fw-medium">Rings</p>
                        </div>

                    </div>
                </div>
            </section>

            <section class="py-5">
                <div class="container">
                    <div class="row" id="product"></div>
                </div>
            </section>

        </main>


        <script src="/assets/js/Bootstrap.min.js"></script>
        <script src="/assets/js/script.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

</body>

</html>